@model MvcLabManager.Models.Computer
@{
    ViewData["Title"] = "Atualizar Computador";
}
<h1>@ViewData["Title"]</h1>

<p>Atualização - Computer</p>

<form class="form" asp-controller="Computer" asp-action="Update" asp-route-id="@Model.Id" method="POST">    
    <label for="Ram">Memória Ram</label>
    <input type="text" id="Ram" value="@Model.Ram" name="Ram">

    <label for="Processor">Processador</label>
    <input type="text" id="Processor" value="@Model.Processor" name="Processor">

    <input class="submit" type="submit" value="Atualizar">
</form>